<!DOCTYPE html>
<html lang="en">

<head>
    {{!-- Document Settings --}}
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    {{!-- Page Meta --}}
    <title>{{meta_title}}</title>
    <meta name="description" content="{{meta_description}}" />

    {{!-- Mobile Meta --}}
    <meta name="HandheldFriendly" content="True" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="robots" content="all,follow">
    <meta name="googlebot" content="index,follow,snippet,archive">

    {{!-- Brand icon --}}
    <link rel="shortcut icon" href="{{asset "favicon.ico"}}">

    {{!-- Styles'n'Scripts --}}
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,500,700,800' rel='stylesheet' type='text/css'>

    <!-- Bootstrap and Font Awesome css -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" href="{{asset "css/screen.css"}}" />
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Merriweather:300,700,700italic,300italic|Open+Sans:700,400" />

    <link rel="stylesheet" type="text/css" href="{{asset "css/site.css"}}">

    <link rel="stylesheet" type="text/css" href="{{asset "css/animate.css"}}">
    <link rel="stylesheet" type="text/css" href="{{asset "css/style.livewithbliss.css"}}">
    <link rel="stylesheet" type="text/css" href="{{asset "css/custom.css"}}">

    <link rel="stylesheet" type="text/css" href="{{asset "css/owl.carousel.css"}}">
    <link rel="stylesheet" type="text/css" href="{{asset "css/owl.theme.css"}}">

    <!-- Responsivity for older IE -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    {{!-- Ghost outputs important style and meta data with this tag --}}
    {{ghost_head}}

    {{#is "post,page"}}
    <link rel="bookmark" href="{{url}}" />
    {{else if "tag"}}
    <link rel="bookmark" href="{{url}}" class="tag" />
    {{else}}
    <link rel="bookmark" href="{{@blog.url}}" />
    {{/is}}
</head>
<body class="{{body_class}}">

    <div id="all">

        <header>
            
            {{> headertopbar}}

            {{> headernavbar}}

        </header>

        {{> header-breadcrumb }}

        {{> featured-section }}

		<!--<div id="contenttest">
			<div class="container">
				<div class="row">


				</div>
			</div>
		</div>-->

        <main class="blog-wrapper" role="main">
            <!--<header class="blog-header">
                <nav class="blog-nav">
                    {{navigation}}
                </nav>
            </header>-->
            <div class="page-wrapper">
                {{{body}}}
                <div class="clearfix"></div>
            </div>
        </main>
        

        <div id="get-it">
			<div class="container">
				<div class="col-md-8 col-sm-12">
					<h3>Do you want a FREE market report?</h3>
				</div>
				<div class="col-md-4 col-sm-12">
					<a href="#" class="btn btn-template-transparent-primary">Download Report</a>
				</div>
			</div>
		</div>

        {{> footer}}

    </div>


    {{!-- jQuery needs to come before `{{ghost_foot}}` so that jQuery can be used in code injection --}}
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    
    <script>
        if (typeof jQuery == 'undefined') {
            //document.write(unescape("%3Cscript src='/js/jquery-3.1.1.min.js' type='text/javascript'%3E%3C/script%3E"));
            document.write(unescape("%3Cscript src='{{asset "js/jquery-3.1.1.min.js"}}' type='text/javascript'%3E%3C/script%3E"));
        }
    </script>
    <!--<script src="https://code.jquery.com/jquery-migrate-3.0.0.min.js"></script>-->
    <script type="text/javascript" src="{{asset "js/jquery-migrate-3.0.0.min.js"}}"></script>

    {{!-- Ghost outputs important scripts and data with this tag --}}
    {{ghost_foot}}

